@{
    string location = string.Format( "{0}://{1}/", Context.Request.Scheme, Context.Request.Host );
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Signed out</title>
</head>
<body>
    <script>
        (function () {
            window.parent.postMessage(JSON.stringify(
            {
                type: 'signedOut'
            }), '@location');
        })();
    </script>
</body>
</html>